<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/bootstrap-grid.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-grid.rtl.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.rtl.css" rel="stylesheet" />
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/Scripts/react/react.js"></script>
    <script src="~/Scripts/react/react.min.js"></script>
    <script src="~/Scripts/vue.js"></script>
    <script src="~/Scripts/vue.min.js"></script>
    <link href="~/Content/NewSheet.css" rel="stylesheet" />
    <title>@ViewBag.Tottenham</title>
</head>
<body>
    <div class="row">
        <div class="col-md-1">
            <img src="~/Images/Tottenham hotspurs 12.png"  width="200" height="200"/>
        </div>
    </div>
    <div>
        <div class="row">
            <div class="col-md-6">
                <nav style="color:blue">
                    @Html.ActionLink("Home","Home","Home")
                    @Html.ActionLink("Gallery","Gallery","Home")
                    @Html.ActionLink("History","History","Home")
                    @Html.ActionLink("Video","Video","Home")
                </nav>
            </div>
        </div>
        @RenderBody()
    </div>
</body>
</html>
